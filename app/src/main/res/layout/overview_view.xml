<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <Switch
        android:id="@+id/switch1"
        android:layout_width="391dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="sans-serif-condensed-light"
        android:scaleX="1.0"
        android:scaleY="1.0"
        android:screenOrientation="portrait"
        android:scrollHorizontally="true"
        android:text="Enable Foreground"
        android:textColorLink="#9C27B0"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/switch2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ov" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="394dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="31dp"
        android:fontFamily="sans-serif-condensed-light"
        android:scaleX="1.0"
        android:scaleY="1.0"
        android:scrollHorizontally="true"
        android:text="Send Data to Server"
        android:textColorLink="#9C27B0"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/sc_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch1" />

    <Button
        android:id="@+id/send_server"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginBottom="36dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="Send to Server"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/ov"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="109dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="110dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="Overview"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sc_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="25dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="Overall Social Distancing Score"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/over_sdScore"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch2" />

    <TextView
        android:id="@+id/over_sdScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="155dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="171dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="69"
        android:textColor="#020202"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/send_server"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sc_label" />

    <TextView
        android:id="@+id/land_sd_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="130dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="69"
        android:textColor="#020202"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/send_server"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/over_sdScore"
        app:layout_constraintTop_toBottomOf="@+id/sc_label" />

    <TextView
        android:id="@+id/person_sd_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="214dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="69"
        android:textColor="#020202"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/over_sdScore"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sc_label" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="57dp"
        android:layout_height="49dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="167dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/person_sd_score"
        app:srcCompat="@drawable/loc_image" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="54dp"
        android:layout_height="47dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="83dp"
        app:layout_constraintBottom_toTopOf="@+id/send_server"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/over_sdScore"
        app:srcCompat="@drawable/overall_score" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="71dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="75dp"
        app:layout_constraintBottom_toTopOf="@+id/send_server"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/land_sd_score"
        app:srcCompat="@drawable/blue_image" />

    <TextView
        android:id="@+id/totalContacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
